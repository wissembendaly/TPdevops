{
    "family": "mini-evax",
    "containerDefinitions": [
        {
            "name": "mini-evax",
            "image": "wissembendaly/mini-evax",
            "portMappings": [
                {
                    "containerPort": 4200,
                    "hostPort": 4200,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512"
}
